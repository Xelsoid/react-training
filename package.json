    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cypress": "^4.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "0.9.0",
    "module-alias": "2.2.2",
    "node-sass": "4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "eslint-plugin-react-hooks": "^4.0.4",
    "redux-mock-store": "^1.5.4",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "development": "cross-env NODE_ENV=development node server.js",
    "production": "cross-env NODE_ENV=production webpack",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:update-snapshots": "jest --updateSnapshot",
    "cypress:e2e": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xelsoid/react-training.git"
  },
  "author": "Artsiom Reutovich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Xelsoid/react-training/issues"
  },
  "homepage": "https://github.com/Xelsoid/react-training#readme",
  "_moduleAliases": {
    "@root": ".",
    "@images": "src/images",
    "@utils": "src/utils",
    "@mockedData": "src/mockedData",
    "@styles": "src/styles",
    "@components": "src/components",
    "@store": "src/store",
    "@reducers": "src/services/reducers.js",
    "@api": "src/services/api.js"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "identity-obj-proxy",
      "^@root(.*)$": "<rootDir>/$1",
      "^@images(.*)$": "<rootDir>/src/images$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@mockedData(.*)$": "<rootDir>/src/mockedData$1",
      "^@styles(.*)$": "<rootDir>/src/styles$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@store(.*)$": "<rootDir>/src/store$1",
      "^@reducers(.*)$": "<rootDir>/src/services/reducers.js",
      "^@api(.*)$": "<rootDir>/src/services/api.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.(js|ts|tsx)?$"
  }
}
